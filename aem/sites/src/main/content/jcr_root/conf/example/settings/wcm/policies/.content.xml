<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <wcm jcr:primaryType="nt:unstructured">
        <foundation jcr:primaryType="nt:unstructured">
            <components jcr:primaryType="nt:unstructured">
                <page jcr:primaryType="nt:unstructured">
                    <jcr:content
                        jcr:primaryType="nt:unstructured"
                        mergeList="{Boolean}true"/>
                    <default
                        jcr:description="Policy of the default page"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Default Page"
                        sling:resourceType="wcm/core/components/policy/policy"
                        clientlibs="[wcm.foundation.components.page.responsive]">
                        <jcr:content
                            jcr:primaryType="nt:unstructured"
                            mergeList="{Boolean}true"/>
                    </default>
                </page>
                <responsivegrid jcr:primaryType="nt:unstructured">
                    <jcr:content
                        jcr:primaryType="nt:unstructured"
                        mergeList="{Boolean}false"/>
                    <default
                        jcr:description="Policy of the default root parsys"
                        jcr:lastModified="{Date}2020-02-25T10:24:58.464+01:00"
                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Example policy"
                        sling:resourceType="wcm/core/components/policy/policy"
                        components="[/libs/wcm/foundation/components/responsivegrid,group:Example Group]">
                        <cq:authoring jcr:primaryType="nt:unstructured">
                            <assetToComponentMapping jcr:primaryType="nt:unstructured"/>
                        </cq:authoring>
                        <jcr:content
                            jcr:primaryType="nt:unstructured"
                            mergeList="{Boolean}true"/>
                    </default>
                </responsivegrid>
            </components>
        </foundation>
    </wcm>
</jcr:root>
